#!/bin/sh

# Source: http://kubernetes.io/docs/getting-started-guides/kubeadm/

sudo mkdir /vora
sudo unzip /vagrant/VORA.ZIP -d /vora
chmod 755 -R /vora
chown vagrant:vagrant -R /vora

masterrunning = $(kubectl get nodes | grep master)
echo $masterrunning

nohup kubectl proxy --address=10.11.12.10 --port=8001 --accept-hosts='^*$' &

sleep 10

exit
